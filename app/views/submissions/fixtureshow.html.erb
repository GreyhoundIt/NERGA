      <th>First Name</th>
      <th>Last Name</th>
      <th>Handicap</th>
      <th>Club</th>
      <th>Venue</th>
      <th>Position</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submissions.each do |submission| %>
      <tr>
