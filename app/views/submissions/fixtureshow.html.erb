        <td><%= link_to 'Show', submission, :class=>" btn1 btn btn-primary " %></td>
        <td><%= link_to 'Edit', edit_submission_path(submission), :class=>" btn1 btn btn-info " %></td>
        <td><%= link_to 'Destroy', submission, method: :delete, data: { confirm: 'Are you sure?' } ,:class=>"btn1 btn btn-danger " %></td>
